
#include <iostream>
using namespace std;



class Sample {
public:
    void printText1()
    {
        cout << "IncludeHelp.com\n";
    }
    void printText2()
    {
        cout << "Let's learn together\n";
    }
    void printValue(int value)
    {
        cout << "value is: " << value << "\n";
    }
};

int main()
{
    Sample obj;

 
    obj.printText1();
    obj.printText2();
    obj.printValue(101);

    return 0;
}